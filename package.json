{
  "name": "react-router-hono-server",
  "version": "2.0.0",
  "description": "This package includes helper function to create an Hono app in your entry.server.tsx file. It allows you to customize your server.",
  "exports": {
    "./node": {
      "import": {
        "import": "./dist/node.mjs",
        "types": "./dist/node.d.mts",
        "default": "./dist/node.mjs"
      }
    },
    "./dev": {
      "import": {
        "import": "./dist/dev.mjs",
        "types": "./dist/dev.d.mts",
        "default": "./dist/dev.mjs"
      }
    },
    "./middlewares": {
      "import": {
        "import": "./dist/middlewares.mjs",
        "types": "./dist/middlewares.d.mts",
        "default": "./dist/middlewares.mjs"
      }
    }
  },
  "scripts": {
    "test": "vitest run",
    "test:cov": "vitest run --coverage",
    "build": "tsup",
    "build:watch": "npm run build -- --watch",
    "example:dev": "npm run dev -w react-router-example",
    "dev": "npm-run-all -s build -p example:dev build:watch",
    "prepublishOnly": "npm run build",
    "check": "biome check .",
    "check:fix": "biome check --fix .",
    "typecheck": "tsc",
    "validate": "npm run check && npm run tsc"
  },
  "author": "",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rphlmr/react-router-hono-server.git"
  },
  "bugs": {
    "url": "https://github.com/rphlmr/react-router-hono-server/issues"
  },
  "bin": {
    "react-router-hono-server": "dist/cli.mjs"
  },
  "files": ["dist"],
  "workspaces": [".", "examples/*"],
  "homepage": "https://github.com/rphlmr/react-router-hono-server#readme",
  "dependencies": {
    "@drizzle-team/brocli": "^0.11.0",
    "@hono/node-server": "^1.13.7",
    "@hono/vite-dev-server": "^0.17.0",
    "hono": "^4.6.11"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/node": "^22.9.3",
    "@vitest/coverage-v8": "^2.1.5",
    "lefthook": "^1.8.4",
    "npm-run-all": "^4.1.5",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2",
    "vitest": "^2.1.5"
  },
  "peerDependencies": {
    "react-router": "^7.0.1",
    "vite": "^5.4.0"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
